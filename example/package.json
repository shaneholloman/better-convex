{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "codegen": "better-convex codegen",
    "convex:dev": "convex dev --until-success --run init && better-convex dev",
    "convex:logs": "bun convex logs --history 50",
    "dev": "mprocs",
    "dev:app": "next dev --port 3005",
    "gen": "next typegen",
    "gen:auth": "@better-auth/cli generate -y --output convex/functions/authSchema.ts --config convex/functions/auth.ts",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "reset": "convex run reset:reset && sleep 5 && convex run init",
    "seed": "convex run seed:seed",
    "start": "next start",
    "studio": "bun convex dashboard",
    "sync": "better-convex env sync --auth",
    "sync:force": "better-convex env sync --auth --force",
    "sync:jwks": "convex run auth:getLatestJwks | convex env set JWKS",
    "sync:plate": "node tooling/sync-plate.cjs",
    "sync:rotate": "convex run auth:rotateKeys | convex env set JWKS",
    "typecheck": "tsc --noEmit && bun run typecheck:convex",
    "typecheck:convex": "tsc --noEmit --project convex/tsconfig.json",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@convex-dev/aggregate": "0.2.1",
    "@convex-dev/rate-limiter": "0.3.2",
    "@convex-dev/react-query": "0.1.0",
    "@convex-dev/resend": "0.2.3",
    "@hookform/resolvers": "5.2.2",
    "@polar-sh/better-auth": "^1.6.4",
    "@polar-sh/sdk": "^0.42.5",
    "@react-email/components": "1.0.6",
    "@react-email/render": "2.0.4",
    "@t3-oss/env-nextjs": "0.13.10",
    "agentation": "^1.3.2",
    "better-convex": "workspace:*",
    "buffer": "^6.0.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "common-tags": "1.8.2",
    "convex": "1.31.7",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "input-otp": "1.4.2",
    "jotai-x": "2.3.3",
    "lucide-react": "0.563.0",
    "next-themes": "0.4.6",
    "nuqs": "2.8.6",
    "radix-ui": "latest",
    "react-day-picker": "9.13.0",
    "react-hook-form": "7.71.1",
    "react-resizable-panels": "3.0.6",
    "recharts": "2.15.4",
    "sonner": "2.0.7",
    "superjson": "2.2.6",
    "tailwind-merge": "3.4.0",
    "ts-essentials": "10.1.1",
    "tsx": "4.21.0",
    "type-fest": "5.4.2",
    "use-debounce": "10.1.0",
    "vaul": "1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/common-tags": "1.8.4",
    "babel-plugin-react-compiler": "1.0.0",
    "mprocs": "^0.8.3",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0"
  }
}
